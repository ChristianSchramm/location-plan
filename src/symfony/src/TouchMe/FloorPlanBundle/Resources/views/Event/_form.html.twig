<div class="row">
  <div class="col-md-4">
    {{ form_start(form) }}
        {{ form_errors(form) }}

        <div class="form-group ">
          {{ form_row(form.title) }}
        </div>
        <div class="form-group row">
          <div  class="col-md-6">
            {{ form_row(form.startdate) }}
          </div>
          <div  class="col-md-6"> 
            {{ form_row(form.starttime) }}
          </div>
        </div>

        <div class="form-group row">
          <div  class="col-md-6">
            {{ form_row(form.enddate) }}
          </div>
          <div  class="col-md-6"> 
            {{ form_row(form.endtime) }}
          </div>
        </div>

        <div class="form-group">
            {{ form_widget(form.save) }}
        </div>
    {{ form_end(form) }}
  </div>
</div>